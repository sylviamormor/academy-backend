{
  "name": "academy-backend",
  "version": "1.0.0",
  "description": "a server app for a bootcamp web platform",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha 'tests/**/*.test.js'",
    "coverage": "nyc mocha tests/**/*.js",
    "start": "nodemon index.js",
    "migrate:create": "db-migrate create --config database.json -e dev",
    "migrate:up": "db-migrate up --config database.json -e dev",
    "migrate:down": "db-migrate down -c 2000 --config database.json -e dev",
    "seed:create": "db-migrate create:seeds --config database.json -e dev",
    "seed:up": "node migrations/seeds/up.admin.seed.js",
    "seed:down": "node migrations/seeds/down.admin.seed.js",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "db-migrate down -c 200 -e test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Albert-Osei/academy-backend.git"
  },
  "keywords": [
    "backend",
    "academy",
    "bootcamp"
  ],
  "author": "albert0711",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Albert-Osei/academy-backend/issues"
  },
  "homepage": "https://github.com/Albert-Osei/academy-backend#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "chai": "^4.3.8",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-enforces-ssl": "^1.1.0",
    "express-fileupload": "^1.4.1",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "sinon": "^16.0.0",
    "supertest": "^6.3.3"
  }
}
